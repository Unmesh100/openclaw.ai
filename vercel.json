{
  "rewrites": [
    {
      "source": "/",
      "destination": "/trust",
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "trust.openclaw.ai"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://openclaw.ai/$1",
      "permanent": true,
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "clawd.bot"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "https://openclaw.ai/$1",
      "permanent": true,
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "www.clawd.bot"
        }
      ]
    }
  ]
}
